{
    "manifest_version": 3,
    "name": "AR Productivity Mascot",
    "version": "1.0",
    "permissions": ["storage", "tabs", "scripting", "alarms"],
    "host_permissions": ["<all_urls>"],
    "background": { "service_worker": "background.js" },
    "action": { "default_popup": "popup.html" }
  }